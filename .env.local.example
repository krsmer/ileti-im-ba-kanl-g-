# Appwrite Configuration
# Appwrite Cloud endpoint veya kendi sunucunuzun endpoint'i
NEXT_PUBLIC_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# Appwrite Console'dan alacağınız Project ID
NEXT_PUBLIC_APPWRITE_PROJECT_ID=your_project_id_here

# Appwrite'da oluşturacağınız Database ID
NEXT_PUBLIC_APPWRITE_DATABASE_ID=your_database_id_here

# Activities Collection ID
NEXT_PUBLIC_APPWRITE_ACTIVITIES_COLLECTION_ID=your_activities_collection_id_here

# Users Collection ID (kullanıcı profilleri için)
NEXT_PUBLIC_APPWRITE_USERS_COLLECTION_ID=your_users_collection_id_here
